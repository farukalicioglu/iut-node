<!DOCTYPE html>
<html lang="en">
<meta http-equiv="content-type" content="text/html;charset=UTF-8" />

<head>
  <%-include('./inc/header-before-css')%>

  <link rel="stylesheet" type="text/css" href="../admin-css/bootstrap.min.css">

  <link rel="stylesheet" href="../admin-css/waves.min.css" type="text/css" media="all">

  <link rel="stylesheet" type="text/css" href="../admin-css/feather.css">

  <link rel="stylesheet" type="text/css" href="../admin-css/themify-icons.css">

  <link rel="stylesheet" type="text/css" href="../admin-css/icofont.css">

  <link rel="stylesheet" type="text/css" href="../admin-css/font-awesome.min.css">

  <link rel="stylesheet" type="text/css" href="../admin-css/style.css">
  <link rel="stylesheet" type="text/css" href="../admin-css/pages.css">
</head>

<body>

  <div class="loader-bg">
    <div class="loader-bar"></div>
  </div>

  <div id="pcoded" class="pcoded">
    <div class="pcoded-overlay-box"></div>
    <div class="pcoded-container navbar-wrapper">

      <%-include('./inc/top-navbar')%>

      <div class="pcoded-main-container">
        <div class="pcoded-wrapper">

          <%-include('./inc/left-nav')%>

          <div class="pcoded-content">

            <div class="page-header card">
              <div class="row align-items-end">
                <div class="col-lg-8">
                  <div class="page-header-title">
                    <i class="feather icon-clipboard bg-c-blue"></i>
                    <div class="d-inline">
                      <h5>yeni sayfa girişi</h5>
                    </div>
                  </div>
                </div>
                <div class="col-lg-4">
                  <div class="page-header-breadcrumb">
                    <ul class=" breadcrumb breadcrumb-title">
                      <li class="breadcrumb-item">
                        <a href="index.html"><i class="feather icon-home"></i></a>
                      </li>
                      <li class="breadcrumb-item"><a href="#!">Form Components</a>
                      </li>
                      <li class="breadcrumb-item">
                        <a href="#!">Forms Validation</a>
                      </li>
                    </ul>
                  </div>
                </div>
              </div>
            </div>

            <div class="pcoded-inner-content">
              <div class="main-body">
                <div class="page-wrapper">

                  <div class="page-body">
                    <div class="row">
                      <div class="col-sm-12">

                        <div class="card">
                          <div class="card-header">
                            <h5>yeni sayfa bileşenleri</h5>
                          </div>
                          <div class="card-block">
                            <form method="post" action="/admin/add-new-pages" novalidate>
                              <div class="form-group row">
                                <label class="col-sm-2 col-form-label">başlık (tr)</label>
                                <div class="col-sm-10">
                                  <input type="text" class="form-control" name="titleTr" id="titleTr" placeholder="türkçe sayfa başlığını giriniz">
                                </div>
                              </div>
                              <div class="form-group row">
                                <label class="col-sm-2 col-form-label">başlık (en)</label>
                                <div class="col-sm-10">
                                  <input type="text" class="form-control" name="titleEn" id="titleEn" placeholder="ingilizce sayfa başlığını giriniz">
                                </div>
                              </div>
                              <div class="form-group row">
                                <label class="col-sm-2 col-form-label">url</label>
                                <div class="col-sm-10">
                                  <input type="text" class="form-control" name="url" id="url" placeholder="url giriniz">
                                </div>
                              </div>
                              <div class="form-group row">
                                <label class="col-sm-2 col-form-label">ejs dosya adı</label>
                                <div class="col-sm-10">
                                  <input type="text" class="form-control" name="ejsFile" id="ejsFile" placeholder="ejs dosya adı giriniz">
                                </div>
                              </div>
                              <div class="form-group row">
                                <label class="col-sm-2 col-form-label">sayfa dili</label>
                                <div class="col-sm-10">
                                  <input type="text" class="form-control" name="lang" id="lang" placeholder="dil giriniz">
                                </div>
                              </div>
                              <div class="form-group row">
                                <label class="col-sm-2 col-form-label">açıklama (tr)</label>
                                <div class="col-sm-10">
                                  <input type="text" class="form-control" name="descriptionTr" id="descriptionTr" placeholder="açıklama giriniz">
                                </div>
                              </div>
                              <div class="form-group row">
                                <label class="col-sm-2 col-form-label">anahtar kelimeler (tr)</label>
                                <div class="col-sm-10">
                                  <input type="text" class="form-control" name="keywordsTr" id="keywordsTr" placeholder="anahtar kelimeleri giriniz">
                                </div>
                              </div>
                              <div class="form-group row">
                                <label class="col-sm-2 col-form-label">açıklama (en)</label>
                                <div class="col-sm-10">
                                  <input type="text" class="form-control" name="descriptionEn" id="descriptionEn" placeholder="açıklama giriniz">
                                </div>
                              </div>
                              <div class="form-group row">
                                <label class="col-sm-2 col-form-label">anahtar kelimeler (en)</label>
                                <div class="col-sm-10">
                                  <input type="text" class="form-control" name="keywordsEn" id="keywordsEn" placeholder="anahtar kelimeleri giriniz">
                                </div>
                              </div>



                              <div class="form-group row">
                                <label class="col-sm-2"></label>
                                <div class="col-sm-10">
                                  <button type="submit" class="btn btn-primary m-b-0">kaydet</button>
                                </div>
                              </div>
                            </form>
                          </div>
                        </div>

                      </div>
                    </div>
                  </div>

                </div>
              </div>
            </div>
          </div>

          <div id="styleSelector">
          </div>
        </div>
      </div>
    </div>
  </div>


<%-include('./inc/internet-explorer')%>


  <script type="text/javascript" src="../admin-js/jquery.min.js"></script>
  <script type="text/javascript" src="../admin-js/jquery-ui.min.js"></script>
  <script type="text/javascript" src="../admin-js/popper.min.js"></script>
  <script type="text/javascript" src="../admin-js/bootstrap.min.js"></script>

  <script src="../admin-js/waves.min.js" type="text/javascript"></script>

  <script type="text/javascript" src="../admin-js/jquery.slimscroll.js"></script>

  <script type="text/javascript" src="../admin-js/modernizr.js"></script>
  <script type="text/javascript" src="../admin-js/css-scrollbars.js"></script>

  <script src="../admin-js/underscore-min.js" type="text/javascript"></script>
  <script src="../admin-js/moment.min.js" type="text/javascript"></script>
  <script type="text/javascript" src="../admin-js/validate.js"></script>

  <script type="text/javascript" src="../admin-js/form-validation.js"></script>
  <script src="../admin-js/pcoded.min.js" type="text/javascript"></script>
  <script src="../admin-js/vertical-layout.min.js" type="text/javascript"></script>
  <script src="../admin-js/jquery.mcustomscrollbar.concat.min.js" type="text/javascript"></script>
  <script type="text/javascript" src="../admin-js/script.js"></script>


</html>
